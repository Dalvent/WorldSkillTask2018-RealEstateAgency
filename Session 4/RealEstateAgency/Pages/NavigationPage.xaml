<Page x:Class="RealEstateAgency.NavigationPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RealEstateAgency"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NavigationPage">

    <Grid>
        <StackPanel>
            <Button Width="200" Name="ReltorButton" Content="Риелторы" Click="ReltorButton_Click"/>
            <Button Width="200" Name="ClientButton" Content="Клиенты" Click="ClientButton_Click"/>
            <Button Width="200" Name="EstleButton" Content="Недвижимость" Click="EstleButton_Click"/>
            <Button Width="200" Name="SupplyButton" Content="Предложение" Click="SupplyButton_Click"/>
            <Button Width="200" Name="DemandButton" Content="Потребность" Click="DemandButton_Click"/>
            <Button Width="200" Name="DealButton" Content="Сделка" Click="DealButton_Click"/>
        </StackPanel>
    </Grid>
</Page>
